# http://editorconfig.org
root = true

[*.{php,py,vue}]
indent_style = space
indent_size = 4

[*.{jsx,tsx}]
indent_size = 4
indent_style = space

[*.{js,ts}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
